<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <defaultCache maxEntriesLocalHeap="20"
                  eternal="false"
                  timeToIdleSeconds="120"
                  timeToLiveSeconds="200"
                  memoryStoreEvictionPolicy="LRU"/>
    <cache name="com.github.com.github.smthyellow.project0.dao.util.part.entity.AuthUserEntity"
           maxEntriesLocalHeap="20"
            eternal="true">
    </cache>
    <cache name="com.github.com.github.smthyellow.project0.dao.util.part.entity.UserEntity"
           maxEntriesLocalHeap="20"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="org.hibernate.cache.StandardQueryCache"
           maxEntriesLocalHeap="100"
            eternal="false"
            timeToLiveSeconds="120">
        <persistence strategy="localTempSwap"/>
    </cache>
</ehcache>